<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.dialect">org.hibernate.dialect.Oracle10gDialect
		</property>
		<property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
		<property name="hibernate.connection.url">jdbc:oracle:thin:@localhost:1521:xe</property>
		<property name="hibernate.connection.username">REPORTING_1_13_X</property>
		<property name="hibernate.connection.password">REPORTING_1_13_X</property>

		<property name="hibernate.connection.autocommit">false</property>
		<property name="hibernate.temp.use_jdbc_metadata_defaults">false</property>
		<property name="hibernate.show_sql">false</property>
		<property name="hibernate.format_sql">true</property>
	</session-factory>
</hibernate-configuration>
